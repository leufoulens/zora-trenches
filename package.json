{
  "name": "zora-trenches",
  "version": "1.0.0",
  "description": "Мониторинг новых создателей на Zora с уведомлениями в Telegram",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "test-x-api": "ts-node src/test-x-api.ts",
    "test-farcaster": "ts-node src/test-farcaster.ts",
    "test-buttons": "ts-node src/test-buttons.ts",
    "test-zora-vc-following": "ts-node src/test.ts",
    "test-alpha": "ts-node src/test-alpha.ts",
    "test-apis": "npm run test-x-api && npm run test-farcaster",
    "test-all": "npm run test-apis && npm run test-buttons",
    "clear-cache": "node -e \"require('./dist/redis-client').RedisClient.prototype.clearProcessedAddresses()\"",
    "test": "echo \"No tests available\" && exit 0"
  },
  "keywords": ["zora", "graphql", "telegram", "redis", "monitoring"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "redis": "^4.6.0",
    "https-proxy-agent": "^7.0.0",
    "node-telegram-bot-api": "^0.64.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-telegram-bot-api": "^0.64.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0"
  }
} 